{
  "name": "adonis-gateway",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@japa/expect": "^1.1.4",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@types/sinon": "^10.0.13",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^9.0.1",
    "prettier": "^2.7.1",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.2",
    "@adonisjs/bouncer": "^2.3.0",
    "@adonisjs/core": "^5.8.6",
    "@adonisjs/limiter": "^1.0.1",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/redis": "^5.0.9",
    "@adonisjs/repl": "^3.1.11",
    "@melchyore/adonis-cache": "^1.0.4",
    "adonis5-jwt": "^1.1.7",
    "adonis5-memcached-client": "^1.1.0",
    "adonis5-prometheus": "^0.0.6",
    "adonis5-swagger": "^1.4.1",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
    "jsonschema": "^1.4.1",
    "luxon": "^3.0.1",
    "nock": "^13.2.9",
    "pg": "^8.8.0",
    "phc-argon2": "^1.1.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.0.11"
  }
}
